{
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"build/**",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/auth",
            "dest":"index.js"
        },
        {
            "src":"/auth/login",
            "dest":"index.js"
        },
        {
            "src":"/auth/register",
            "dest":"index.js"
        },
        {
            "src":"/users",
            "dest":"index.js"
        },
        {
            "src":"/users/(.*)",
            "dest":"index.js"
        },
        {
            "src":"/users/(.*)/friends",
            "dest":"index.js"
        },
        {
            "src":"/users/(.*)/(.*)",
            "dest":"index.js"
        },
        {
            "src":"/posts",
            "dest":"index.js"
        },
        {
            "src":"/posts/(.*)/posts",
            "dest":"index.js"
        },
        {
            "src":"/posts/(.*)/like",
            "dest":"index.js"
        },
        {
            "src":"/posts/(.*)/comment",
            "dest":"index.js"
        },
        {
            "src":"/",
            "dest":"build/index.html"
        },
        {
            "src":"/(.+)",
            "dest":"build/$1"
        }
    ]
}