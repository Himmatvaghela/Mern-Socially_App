{
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"build/**",
            "use":"@vercel/static"
        }
    ],
    "functions": {
        "auth/register": {
            "handler": "server/serverless/upload.js"
        },
        "posts": {
            "handler": "server/serverless/upload.js"
        }
    },
    "rewrites": [{ "source": "/(.*)", "destination": "index.js" }]
}